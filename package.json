{
  "name": "when-lambo",
  "version": "1.0.20",
  "description": "The bot that will make us rich",
  "author": "Two beautiful guys",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "dev": "rm -rf dist && tsc && NODE_ENV=development node dist/index | ./node_modules/.bin/bunyan",
    "start": "node dist/index.js",
    "predeploy": "npm version patch --no-git-tag-version && PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git commit -a -m \"Publish version $PACKAGE_VERSION\" && git push origin master && git checkout develop && git rebase master && git push origin develop && git checkout master",
    "deploy": "npx pm2 deploy one update && npx pm2 deploy two update && npx pm2 deploy three update && npx pm2 deploy four update",
    "tsc": "tsc",
    "lint": "eslint .",
    "format": "npm-run-all format:*",
    "format:json": "prettier --write \"**/*.json\"",
    "format:ts": "prettier --write \"**/*.ts\""
  },
  "dependencies": {
    "@aws/web3-ws-provider": "^1.0.3",
    "@maxime.julian/ethereum-multicall": "^3.0.0",
    "aws-sdk": "^2.1016.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "bunyan": "^1.8.15",
    "bunyan-rotating-file-stream": "^2.0.0",
    "date-fns": "^2.25.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.5.1",
    "fastify": "^3.24.0",
    "module-alias": "^2.2.2",
    "serialize-error": "^8.1.0",
    "web3": "^1.6.0",
    "web3-eth-contract": "^1.6.0",
    "web3-providers-ws": "^1.6.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@trivago/prettier-plugin-sort-imports": "^3.0.0",
    "@types/bunyan": "^1.8.8",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/websocket": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "console.table": "^0.10.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "google-spreadsheet": "^3.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "typed-emitter": "^1.4.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run tsc",
      "npm run lint"
    ]
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "_moduleAliases": {
    "@src": "./dist/src"
  }
}
