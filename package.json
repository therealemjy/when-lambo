    "web3": "^1.2.9"
  },
  "devDependencies": {
    "console.table": "^0.10.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=16.13.0"
  }
