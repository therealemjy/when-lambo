{
  "name": "when-lambo",
  "version": "1.0.0",
  "description": "The bot that will make us rich",
  "author": "Two beautiful guys",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build:secure-copy": "scp -i ~/.ssh/when-lambo.pem ./build.zip ubuntu@ec2-3-91-241-119.compute-1.amazonaws.com:~/data",
    "build": "tsc && npm run build:add-dependencies && npm run build:zip",
    "build:add-dependencies": "rm -rf dist/node_modules && npm ls --only prod --parseable | tail -n +2 | xargs -n1 sh -c 'for arg do echo \"$arg\"; XDIR=$(echo \"$arg\" | xargs dirname | sed -E \"s/(.+node_modules)+(\\/@.+)?/dist\\/node_modules\\2/g\"); mkdir -p $XDIR; echo $XDIR; done' _ | xargs -n2 cp -r",
    "build:zip": "zip -r build.zip dist/",
    "deploy": "npm run build && npm run build:secure-copy",
    "dev": "tsc && NODE_ENV=development node dist/index",
    "start": "NODE_ENV=production node index.js",
    "tsc": "tsc",
    "lint": "eslint . --ext ts",
    "format": "npm-run-all format:*",
    "format:json": "prettier --write \"**/*.json\"",
    "format:ts": "prettier --write \"**/*.ts\""
  },
  "dependencies": {
    "@aws/web3-ws-provider": "^1.0.3",
    "aws-sdk": "^2.1016.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "console.table": "^0.10.0",
    "date-fns": "^2.25.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.5.1",
    "google-spreadsheet": "^3.1.15",
    "module-alias": "^2.2.2",
    "ts-node": "^10.4.0",
    "web3": "^1.6.0",
    "web3-eth-contract": "^1.6.0",
    "web3-providers-ws": "^1.6.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@trivago/prettier-plugin-sort-imports": "^3.0.0",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/websocket": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "_moduleAliases": {
    "@src": "./dist/src"
  }
}
