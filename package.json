{
  "name": "when-lambo-smart-contracts",
  "version": "1.0.33",
  "description": "Smart contracts for When Lambo",
  "author": "",
  "private": true,
  "main": "bot/dist/bot/index.js",
  "scripts": {
    "tsc": "npm run bot:tsc && tsc --noEmit",
    "lint": "eslint .",
    "test": "NODE_ENV=test hardhat test | ./node_modules/.bin/bunyan",
    "format": "npm-run-all format:*",
    "format:json": "prettier --write \"**/*.json\"",
    "format:ts": "prettier --write \"**/*.ts\"",
    "fetch-gas-estimates": "ts-node scripts/fetchGasEstimates/index.ts | ./node_modules/.bin/bunyan",
    "bot:tsc": "tsc --noEmit --project ./bot/tsconfig.json",
    "bot:build": "rm -rf ./dist/bot/index.js && esbuild ./bot/index.ts --bundle --outfile=./dist/bot/index.js --platform=node",
    "bot:dev": "npm run bot:build && NODE_ENV=development node --inspect ./dist/bot/index | ./node_modules/.bin/bunyan",
    "communicator:build": "rm -rf ./dist/communicator/index.js && esbuild ./communicator/index.ts --bundle --outfile=./dist/communicator/index.js --platform=node",
    "communicator:dev": "npm run communicator:build && NODE_ENV=development node --inspect ./dist/communicator/index | ./node_modules/.bin/bunyan",
    "prebot:deploy": "npm run tsc && npm run lint && npm run test && USE_PROD_STRATEGIES=true npm run fetch-gas-estimates && npm run bot:build && npm version patch --no-git-tag-version && PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git commit -a -m \"Publish version $PACKAGE_VERSION\" --no-verify && git push origin master && git checkout develop && git rebase master && git push origin develop && git checkout master",
    "bot:deploy": "npx pm2 deploy one update --force",
    "prepare": "husky install"
  },
  "license": "ISC",
  "dependencies": {
    "@aws/web3-ws-provider": "^1.0.3",
    "@maxime.julian/ethereum-multicall": "^3.0.0",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "aws-sdk": "^2.1016.0",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "bunyan": "^1.8.15",
    "bunyan-rotating-file-stream": "^2.0.0",
    "date-fns": "^2.25.0",
    "dotenv": "^8.2.0",
    "ethers": "^5.5.2",
    "google-spreadsheet": "^3.2.0",
    "serialize-error": "^8.1.0",
    "typed-emitter": "^1.4.0",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.11",
    "@nomiclabs/hardhat-etherscan": "^2.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.3",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/bunyan": "^1.8.8",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/mocha": "^9.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.11.10",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "console.table": "^0.10.0",
    "esbuild": "^0.14.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.1",
    "ethereum-waffle": "^3.4.0",
    "google-spreadsheet": "^3.2.0",
    "hardhat": "^2.7.0",
    "hardhat-deploy": "^0.9.10",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.4",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "serialize-error": "^8.1.0",
    "sinon": "^12.0.1",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "typechain": "^5.2.0",
    "typescript": "^4.5.2"
  },
  "_moduleAliases": {
    "@localTypes": "./types",
    "@constants": "./constants",
    "@logger": "./logger",
    "@dist": "./dist",
    "@tasks": "./tasks",
    "@utils": "./utils",
    "@resources": "./resources",
    "@deployments": "./deployments",
    "@chainHandler": "./chainHandler",
    "@bot": "./bot",
    "@communicator": "./communicator"
  }
}
