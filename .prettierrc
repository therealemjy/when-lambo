{
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "importOrder": [
    "./moduleAliases",
    "^@(localTypes|constants|utils|logger|resources|deployments)",
    "^@tasks/(.*)$",
    "^@chainHandler/(.*)$",
    "^@communicator/(.*)$",
    "^@bot/(.*)$",
    "^@dist/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderParserPlugins": ["classProperties", "typescript"]
}
